<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="341.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uq.taller_cola.Procesos.ClienteController">
    <children>
        <Label fx:id="nombreLabel" text="Nombre: " />
      <TextField fx:id="txtNombre" onAction="#nombre" />
        <Label fx:id="idLabel" text="ID: " />
      <TextField fx:id="txtid" onAction="#id" />
        <Label fx:id="prioridadLabel" text="Prioridad: " />
      <ComboBox fx:id="cmbPrioridad" onAction="#prioridadCliente" prefWidth="150.0" />
      <Label alignment="CENTER" contentDisplay="TOP" prefHeight="37.0" prefWidth="189.0" text="SU TURNO ES EL : " />
      <TextField fx:id="txtTurno" />
      <Button mnemonicParsing="false" onAction="#salir" text="SALIR" />
    </children>
</VBox>
